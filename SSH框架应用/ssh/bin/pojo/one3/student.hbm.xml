<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="pojo.one3.student" table="学生表">
    <id name="id">
        <generator class="native"/>
    </id>
    <property name="name" column="姓名"></property>
    <property name="age" column="年龄"></property>
    <property name="stuId" column="学号"></property>
    <property name="dom"></property>
    <many-to-one name="dom" column="宿舍_id" class="pojo.one3.dom"/>
    </class>
</hibernate-mapping>



