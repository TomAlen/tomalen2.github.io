<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="pojo.single2.VipCard" table="vip表">
    <id name="id">
        <generator class="native"/>
    </id>
    <property name="no" column="编号"></property>
    <property name="level" column="等级"></property>
    <one-to-one name="myself" class="pojo.single2.person" />
   </class>
</hibernate-mapping>

